{
  "name": "rails-api-react-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ui:build": "NODE_ENV=production next build react-ui",
    "ui:build:static": "NODE_ENV=production next build react-ui && next export react-ui -o dist",
    "ui:dev": "next dev react-ui -p 3150",
    "ui:start": "next start react-ui",
    "ui:export": "next export react-ui -o dist",
    "ui:format": "prettier --write \"react-ui/src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\"",
    "ui:lint": "next react-ui lint",
    "bundle": "yarn --cwd rails-api bundle",
    "db:setup": ""
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "clsx": "^1.1.1",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "@types/react": "17.0.36",
    "autoprefixer": "^10.2.6",
    "eslint": "8.3.0",
    "eslint-config-next": "12.0.4",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.2.2",
    "typescript": "4.5.2"
  },
  "license": "MIT"
}
